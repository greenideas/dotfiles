--langdef=markdown
--langmap=markdown:.mdown.markdown
--regex-markdown=/^#[ \t]+(.*)/\1/h,Heading_L1/
--regex-markdown=/^##[ \t]+(.*)/\1/i,Heading_L2/
--regex-markdown=/^###[ \t]+(.*)/\1/k,Heading_L3/
--regex-markdown=/^####[ \t]+(.*)/\1/h,Heading_L4/
--regex-markdown=/^#####[ \t]+(.*)/\1/i,Heading_L5/
--regex-markdown=/^######[ \t]+(.*)/\1/k,Heading_L6/
--regex-markdown=/^#######[ \t]+(.*)/\1/k,Heading_L7/
